                            <amp-accordion disable-session-states>
                                <section>
                                    <header class="ca_h daccord_h">
                                        <i class="i i-plus ca_hi" aria-hidden="true"></i>
                                        SMART Vocabulary: related words and
                                        phrases
                                    </header>
                                    <div class="ca_b had daccord_b daccord_l">
                                        <div class="daccord_lt">
                                            <a href="https://dictionary.cambridge.org/topics/technology/internet-terminology-and-abbreviations/"
                                                title="Words and phrases related to pull in the topic Internet terminology">Internet
                                                terminology</a>
                                        </div>

                                        <div class="daccord_lb">

                                            <ul class="hul-u hul-u0 hax lmb-10 lcs">
